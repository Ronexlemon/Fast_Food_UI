<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

<androidx.coordinatorlayout.widget.CoordinatorLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:columnCount="2"
                android:layout_marginTop="8dp"
                android:rowCount="5"
                android:background="@drawable/backitem"
                tools:context=".product.ItemFragment">

                <com.google.android.material.card.MaterialCardView
                    android:layout_width="160dp"
                    android:background="@color/card"
                    android:layout_height="160dp"
                    android:layout_row="0"
                    android:layout_rowWeight="1"
                    android:layout_column="0"
                    android:layout_columnWeight="1"
                    android:layout_margin="8dp"
                    android:padding="4dp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:id="@+id/fries"
                        android:layout_gravity="center"
                        android:contentDescription="@string/fries"
                        android:src="@drawable/eight"
                        app:civ_border_color="@color/black"
                        app:civ_border_width="2dp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fries"
                        android:id="@+id/one"
                        android:layout_gravity="bottom|center_horizontal"/>
                    <CheckBox
                        android:id="@+id/check1"
                        android:checked="true"
                        style="@style/checks"
                        />

                </com.google.android.material.card.MaterialCardView>
                <com.google.android.material.card.MaterialCardView
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:background="@color/card"
                    android:layout_row="0"
                    android:layout_rowWeight="1"
                    android:layout_column="1"
                    android:layout_columnWeight="1"
                    android:layout_margin="8dp"
                    android:padding="4dp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:layout_width="98dp"
                        android:layout_height="102dp"
                        android:layout_gravity="center"
                        android:contentDescription="@string/fries"
                        android:src="@drawable/eight"
                        app:civ_border_color="@color/black"
                        app:civ_border_width="2dp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fries"
                        android:id="@+id/two"
                        android:layout_gravity="bottom|center_horizontal"
                        />
                    <CheckBox
                        android:id="@+id/check2"
                        style="@style/checks"
                        />
                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:layout_row="1"
                    android:background="@color/card"
                    android:layout_rowWeight="1"
                    android:layout_column="1"
                    android:layout_columnWeight="1"
                    android:layout_margin="8dp"
                    android:padding="4dp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:layout_gravity="center"
                        android:contentDescription="@string/fries"
                        android:src="@drawable/eight"
                        app:civ_border_color="@color/black"
                        app:civ_border_width="2dp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fries"
                        android:layout_gravity="bottom|center_horizontal"
                        />
                    <CheckBox
                        android:id="@+id/check3"
                        style="@style/checks"
                        />
                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:layout_row="1"
                    android:background="@color/card"
                    android:layout_rowWeight="1"
                    android:layout_column="0"
                    android:layout_columnWeight="1"
                    android:layout_margin="8dp"
                    android:padding="4dp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:layout_gravity="center"
                        android:contentDescription="@string/fries"
                        android:src="@drawable/eight"
                        app:civ_border_color="@color/black"
                        app:civ_border_width="2dp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fries"
                        android:layout_gravity="bottom|center_horizontal"
                        />
                    <CheckBox
                        android:id="@+id/check4"
                        style="@style/checks"
                        />
                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:layout_row="2"
                    android:background="@color/card"
                    android:layout_rowWeight="1"
                    android:layout_column="1"
                    android:layout_columnWeight="1"
                    android:layout_margin="8dp"
                    android:padding="4dp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:layout_gravity="center"
                        android:contentDescription="@string/fries"
                        android:src="@drawable/eight"
                        app:civ_border_color="@color/black"
                        app:civ_border_width="2dp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fries"
                        android:layout_gravity="bottom|center_horizontal"
                        />
                    <CheckBox
                        android:id="@+id/check6"
                        style="@style/checks"
                        />
                </com.google.android.material.card.MaterialCardView>
                <com.google.android.material.card.MaterialCardView
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:layout_row="2"

                    android:background="@color/card"
                    android:layout_rowWeight="1"
                    android:layout_column="0"
                    android:layout_columnWeight="1"
                    android:layout_margin="8dp"
                    android:padding="4dp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:layout_gravity="center"
                        android:contentDescription="@string/fries"
                        android:src="@drawable/eight"
                        app:civ_border_color="@color/black"
                        app:civ_border_width="2dp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fries"
                        android:layout_gravity="bottom|center_horizontal"
                        android:id="@+id/name3"/>
                    <CheckBox
                        android:id="@+id/check5"
                        style="@style/checks"
                        />
                </com.google.android.material.card.MaterialCardView>
                <com.google.android.material.card.MaterialCardView
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:layout_row="3"
                    android:layout_rowWeight="0"
                    android:layout_column="1"
                    android:background="@color/card"
                    android:layout_columnWeight="1"
                    android:layout_margin="8dp"
                    android:padding="4dp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:layout_gravity="center"
                        android:contentDescription="@string/fries"
                        android:src="@drawable/eight"
                        app:civ_border_color="@color/black"
                        app:civ_border_width="2dp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fries"
                        android:layout_gravity="bottom|center_horizontal"
                        android:id="@+id/name2"/>
                </com.google.android.material.card.MaterialCardView>
                <com.google.android.material.card.MaterialCardView
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:layout_row="3"
                    android:background="@color/card"
                    android:layout_rowWeight="1"
                    android:layout_column="1"
                    android:layout_columnWeight="1"
                    android:layout_margin="8dp"
                    android:padding="4dp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:layout_gravity="center"
                        android:contentDescription="@string/fries"
                        android:src="@drawable/eight"
                        app:civ_border_color="@color/black"
                        app:civ_border_width="2dp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/fries"
                        android:layout_gravity="bottom|center_horizontal"
                        android:id="@+id/name1"/>
                    <CheckBox
                        android:id="@+id/check7"
                        style="@style/checks"
                        />
                </com.google.android.material.card.MaterialCardView>




            </GridLayout>


        </androidx.core.widget.NestedScrollView>
        <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            android:id="@+id/floating_action_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:backgroundTint="@color/purple_200"
            android:layout_margin="20dp"
            android:elevation="10dp"
            android:text="@string/cart1"
            tools:ignore="ObsoleteLayoutParam" />





</androidx.coordinatorlayout.widget.CoordinatorLayout>
</layout>
